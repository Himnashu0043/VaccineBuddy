        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/pink"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tvAgeOfBirth"
            style="@style/normaltxt_12sp_Medium"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginStart="8dp"
            android:gravity="center"
            android:text="Age of Baby"
            android:textColor="@color/newblack"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/divider4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/divider4"
            android:layout_width="1dp"
            android:layout_height="0dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tvInsuions"
            app:layout_constraintStart_toEndOf="@+id/tvAgeOfBirth"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvInsuions"
            style="@style/normaltxt_12sp_Medium"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="Inclusions"
            android:textColor="@color/newblack"
            app:layout_constraintBottom_toBottomOf="@+id/tvAgeOfBirth"
            app:layout_constraintEnd_toStartOf="@+id/divider8"
            app:layout_constraintStart_toEndOf="@+id/divider4"
            app:layout_constraintTop_toTopOf="@+id/tvAgeOfBirth" />

        <View
            android:id="@+id/divider8"
            android:layout_width="1dp"
            android:layout_height="0dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toBottomOf="@+id/divider4"
            app:layout_constraintEnd_toStartOf="@+id/tvStatus"
            app:layout_constraintStart_toEndOf="@+id/tvInsuions"
            app:layout_constraintTop_toTopOf="@+id/divider4" />

        <TextView
            android:id="@+id/tvStatus"
            style="@style/normaltxt_12sp_Medium"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginEnd="8dp"
            android:gravity="center"
            android:text="Status"
            android:textColor="@color/newblack"
            app:layout_constraintBottom_toBottomOf="@+id/tvInsuions"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/divider8"
            app:layout_constraintTop_toTopOf="@+id/tvInsuions" />


