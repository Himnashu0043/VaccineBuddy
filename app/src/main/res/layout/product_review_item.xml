<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_marginBottom="@dimen/dp_8"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView64"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/pr" />

        <TextView
            android:id="@+id/textView174"
            style="@style/normaltxt_12sp_Medium"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="14dp"
            android:layout_marginEnd="32dp"
            android:text="Trendy Shoes For Man With High Quality Fabrics And Breathable Outdoor Sport Sneakers."
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <ImageView
            android:id="@+id/imageView65"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView172"
            app:layout_constraintStart_toStartOf="@+id/textView174"
            app:layout_constraintTop_toTopOf="@+id/textView172"
            app:srcCompat="@drawable/ic_star__1_" />

        <ImageView
            android:id="@+id/imageView651"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginLeft="@dimen/dp_5"
            app:layout_constraintBottom_toBottomOf="@+id/imageView65"
            app:layout_constraintStart_toEndOf="@+id/imageView65"
            app:layout_constraintTop_toTopOf="@+id/imageView65"
            app:srcCompat="@drawable/ic_star__1_" />

        <ImageView
            android:id="@+id/imageView6511"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginStart="5dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/imageView651"
            app:layout_constraintStart_toEndOf="@+id/imageView651"
            app:layout_constraintTop_toTopOf="@+id/imageView651"
            app:srcCompat="@drawable/ic_star__1_" />

        <ImageView
            android:id="@+id/imageView65111"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginStart="5dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/imageView6511"
            app:layout_constraintStart_toEndOf="@+id/imageView6511"
            app:layout_constraintTop_toTopOf="@+id/imageView6511"
            app:srcCompat="@drawable/ic_star__1_" />

        <ImageView
            android:id="@+id/imageView651111"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginStart="5dp"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="@+id/imageView65111"
            app:layout_constraintStart_toEndOf="@+id/imageView65111"
            app:layout_constraintTop_toTopOf="@+id/imageView65111"
            app:srcCompat="@drawable/ic_star" />

        <TextView
            android:id="@+id/textView172"
            style="@style/normaltxt_10sp_Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:text="2 hours ago"
            android:textColor="@color/newblack"
            app:layout_constraintStart_toEndOf="@+id/imageView651111"
            app:layout_constraintTop_toBottomOf="@+id/imageView64" />

        <View
            android:id="@+id/divider2"
            android:layout_width="409dp"
            android:layout_height="1dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="8dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView172" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>