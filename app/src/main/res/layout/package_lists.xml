<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/dp_16"
    android:layout_marginTop="@dimen/dp_8"
    android:layout_marginRight="@dimen/dp_16"
    android:layout_marginBottom="@dimen/dp_6">

    <androidx.cardview.widget.CardView
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        app:cardCornerRadius="@dimen/dp_10"
        app:cardElevation="@dimen/dp_4"
        android:layout_margin="@dimen/dp_4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/imageView18"
                android:layout_width="@dimen/dp_80"
                android:layout_height="@dimen/dp_80"
                android:layout_marginStart="@dimen/dp_12"
                android:layout_marginBottom="@dimen/dp_32"
                android:scaleType="fitXY"
                app:layout_constraintBottom_toBottomOf="@id/layout"
                app:layout_constraintStart_toStartOf="@id/layout"
                app:layout_constraintTop_toTopOf="@+id/tv_wishPackageName"
                app:shapeAppearanceOverlay="@style/service_10_imge_shapeable_overloay" />

            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/lottie"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:scaleType="fitCenter"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@+id/imageView18"
                app:layout_constraintEnd_toEndOf="@+id/imageView18"
                app:layout_constraintStart_toStartOf="@+id/imageView18"
                app:layout_constraintTop_toTopOf="@+id/imageView18"
                app:lottie_autoPlay="true"
                app:lottie_fileName="loading.json"
                app:lottie_loop="true" />

            <TextView
                android:id="@+id/tv_wishPackageName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_12"
                android:layout_marginTop="@dimen/dp_14"
                android:layout_marginEnd="8dp"
                android:fontFamily="@font/gilroy_bold"
                android:gravity="left"
                android:singleLine="true"
                android:textColor="@color/newcolor"
                android:textSize="@dimen/dp_15"
                app:layout_constraintEnd_toStartOf="@+id/ivDelete"
                app:layout_constraintStart_toEndOf="@+id/imageView18"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_wishPackageDes"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_12"
                android:layout_marginTop="@dimen/dp_4"
                android:fontFamily="@font/gilroy_regular"
                android:gravity="left"
                android:maxLines="2"
                android:ellipsize="marquee"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_12"
                app:layout_constraintEnd_toEndOf="@+id/tv_wishPackageName"
                app:layout_constraintStart_toEndOf="@+id/imageView18"
                app:layout_constraintTop_toBottomOf="@+id/tv_wishPackageName" />

            <TextView
                android:id="@+id/tv_wishPackagePrice"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_8"
                android:fontFamily="@font/gilroy_bold"
                android:textColor="@color/newcolor"
                android:textSize="@dimen/sp_15"
                app:layout_constraintStart_toStartOf="@+id/tv_wishPackageDes"
                app:layout_constraintTop_toBottomOf="@+id/tv_wishPackageDes" />
            <TextView
                android:id="@+id/tvwishlistOfferPrice"
                style="@style/normaltxt_15spbold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:visibility="visible"
                app:layout_constraintStart_toEndOf="@+id/tv_wishPackagePrice"
                app:layout_constraintTop_toTopOf="@+id/tv_wishPackagePrice" />

            <View
                android:id="@+id/divider13"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:visibility="visible"
                android:background="@color/black"
                app:layout_constraintBottom_toBottomOf="@+id/tv_wishPackagePrice"
                app:layout_constraintEnd_toEndOf="@+id/tv_wishPackagePrice"
                app:layout_constraintStart_toStartOf="@+id/tv_wishPackagePrice"
                app:layout_constraintTop_toTopOf="@+id/tv_wishPackagePrice" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:fontFamily="@font/gilroy_regular"
                android:gravity="left"
                android:text="@string/included_vaccines_12"
                android:textSize="@dimen/sp_12"
                app:layout_constraintStart_toStartOf="@+id/tv_wishPackagePrice"
                app:layout_constraintTop_toBottomOf="@+id/tv_wishPackagePrice" />

            <TextView
                android:id="@+id/tvIncludeVaccine"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/gilroy_regular"
                android:gravity="left"
                android:textColor="@color/pink"
                android:textSize="@dimen/sp_12"
                app:layout_constraintBottom_toBottomOf="@+id/textView31"
                app:layout_constraintStart_toEndOf="@+id/textView31"
                app:layout_constraintTop_toTopOf="@+id/textView31" />

            <TextView
                android:id="@+id/textView39"
                style="@style/normaltxt_10sp_Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:drawableStart="@drawable/ic_star__1_"
                android:gravity="center"
                android:visibility="gone"
                android:paddingLeft="5dp"
                android:text="@string/_4_0"
                app:layout_constraintBottom_toBottomOf="@+id/tv_wishPackagePrice"
                app:layout_constraintStart_toEndOf="@+id/tv_wishPackagePrice"
                app:layout_constraintTop_toTopOf="@+id/tv_wishPackagePrice" />

            <TextView
                android:id="@+id/textView110"
                style="@style/normaltxt_8sp_Regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_2"
                android:text="@string/_121_reviews"
                android:visibility="gone"
                android:textColor="#CCCCCC"
                app:layout_constraintBottom_toBottomOf="@+id/textView39"
                app:layout_constraintStart_toEndOf="@+id/textView39"
                app:layout_constraintTop_toTopOf="@+id/textView39" />

            <ImageView
                android:id="@+id/ivDelete"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="19dp"
                android:layout_marginEnd="@dimen/dp_16"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_delete" />

            <TextView
                android:id="@+id/tvoutofstock"
                style="@style/normaltxt_12sp_Semibold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="@dimen/dp_10"
                android:gravity="center_horizontal"
                android:text="Out of Stock"
                android:textColor="@color/pink"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/imageView18"
                app:layout_constraintStart_toStartOf="@+id/imageView18"
                app:layout_constraintTop_toBottomOf="@+id/imageView18" />

            <TextView
                android:id="@+id/textView196"
                style="@style/normaltxt_14sp_Medium"
                android:layout_width="@dimen/dp_73"
                android:layout_height="@dimen/dp_30"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="16dp"
                android:background="@drawable/blue_stroke_bg"
                android:gravity="center"
                android:text="Add"
                android:visibility="gone"
                android:textColor="@color/darknavyblue2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>