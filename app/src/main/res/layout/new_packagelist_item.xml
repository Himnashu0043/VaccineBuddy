<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/dp_12"
    android:layout_marginTop="@dimen/dp_4"
    android:layout_marginRight="@dimen/dp_12"
    android:layout_marginBottom="@dimen/dp_4">

    <androidx.cardview.widget.CardView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_4"
        app:cardCornerRadius="@dimen/dp_10"
        app:cardElevation="@dimen/dp_2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/imageView18"
                android:layout_width="@dimen/dp_80"
                android:layout_height="@dimen/dp_80"
                android:layout_marginStart="@dimen/dp_12"
                android:layout_marginTop="@dimen/dp_12"
                android:scaleType="fitXY"
                app:layout_constraintStart_toStartOf="@id/layout"
                app:layout_constraintTop_toTopOf="@id/layout"
                app:shapeAppearanceOverlay="@style/service_10_imge_shapeable_overloay">

            </com.google.android.material.imageview.ShapeableImageView>

            <TextView
                android:id="@+id/tvnewSub"
                style="@style/normaltxt_12sp_bold"
                android:layout_width="0dp"
                android:visibility="invisible"
                android:layout_marginBottom="@dimen/dp_2"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/imageView18"
                app:layout_constraintStart_toStartOf="@+id/imageView18"
                app:layout_constraintTop_toBottomOf="@+id/imageView18" />

            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/lottie"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@+id/imageView18"
                app:layout_constraintEnd_toEndOf="@+id/imageView18"
                app:layout_constraintStart_toStartOf="@+id/imageView18"
                app:layout_constraintTop_toTopOf="@+id/imageView18"
                app:lottie_autoPlay="true"
                app:lottie_fileName="loading.json"
                app:lottie_loop="true" />

            <TextView
                android:id="@+id/tvPackageAllName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_12"
                android:layout_marginTop="@dimen/dp_14"
                android:layout_marginEnd="8dp"
                android:fontFamily="@font/gilroy_bold"
                android:gravity="left"
                android:maxLength="50"
                android:textColor="@color/newcolor"
                android:textSize="@dimen/dp_15"
                app:layout_constraintEnd_toStartOf="@+id/cardView2"
                app:layout_constraintStart_toEndOf="@+id/imageView18"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:visibility="visible"
                app:cardCornerRadius="@dimen/dp_50"
                app:cardElevation="@dimen/dp_3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/ivfav"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/group1"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/tvPackageSub_cat"
                style="@style/normaltxt_14sp_Medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_12"
                android:layout_marginTop="@dimen/dp_4"
                android:maxLength="50"
                android:textColor="@color/black"
                app:layout_constraintEnd_toEndOf="@+id/tvPackageAllName"
                app:layout_constraintStart_toEndOf="@+id/imageView18"
                app:layout_constraintTop_toBottomOf="@+id/tvPackageAllName" />

            <TextView
                android:id="@+id/tvPackageAllDes"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_4"
                android:fontFamily="@font/gilroy_regular"
                android:maxLength="50"
                android:textColor="@color/black"
                android:textSize="@dimen/sp_12"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="@+id/tvPackageAllName"
                app:layout_constraintStart_toStartOf="@+id/tvPackageSub_cat"
                app:layout_constraintTop_toBottomOf="@+id/tvPackageSub_cat" />
            <TextView
                android:id="@+id/tvPackageAllPrice"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_8"
                android:fontFamily="@font/gilroy_bold"
                android:textColor="@color/newcolor"
                android:textSize="@dimen/sp_15"
                android:visibility="visible"
                app:layout_constraintStart_toStartOf="@+id/tvPackageSub_cat"
                app:layout_constraintTop_toBottomOf="@+id/tvPackageAllDes" />

            <TextView
                android:id="@+id/tvPackageOffer_Price"
                style="@style/normaltxt_15spbold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@+id/tvPackageAllPrice"
                app:layout_constraintStart_toEndOf="@+id/tvPackageAllPrice"
                app:layout_constraintTop_toTopOf="@+id/tvPackageAllPrice" />

            <View
                android:id="@+id/divider13"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="@color/black"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@+id/tvPackageAllPrice"
                app:layout_constraintEnd_toEndOf="@+id/tvPackageAllPrice"
                app:layout_constraintStart_toStartOf="@+id/tvPackageAllPrice"
                app:layout_constraintTop_toTopOf="@+id/tvPackageAllPrice" />
            <TextView
                android:id="@+id/textView31"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:fontFamily="@font/gilroy_regular"
                android:gravity="left"
                android:text="@string/included_vaccines_12"
                android:textSize="@dimen/sp_12"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/tvPackageAllPrice"
                app:layout_constraintTop_toBottomOf="@+id/tvPackageAllPrice" />

            <TextView
                android:id="@+id/tvIncludeDose"
                style="@style/normaltxt_12sp_Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="4dp"
                android:textColor="@color/pink"
                app:layout_constraintBottom_toBottomOf="@+id/textView31"
                app:layout_constraintStart_toEndOf="@+id/textView31"
                app:layout_constraintTop_toTopOf="@+id/textView31" />

            <TextView
                android:id="@+id/textView39"
                style="@style/normaltxt_10sp_Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableStart="@drawable/ic_star__1_"
                android:drawablePadding="@dimen/dp_5"
                android:gravity="center"
                android:text="@string/_4_0"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/tvPackageAllPrice"
                app:layout_constraintStart_toStartOf="@+id/tvPackageSub_cat"
                app:layout_constraintTop_toTopOf="@+id/tvPackageAllPrice"
                app:layout_constraintVertical_bias="1.0" />

            <TextView
                android:id="@+id/textView110"
                style="@style/normaltxt_8sp_Regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_8"
                android:text="@string/_121_reviews"
                android:textColor="#CCCCCC"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/textView39"
                app:layout_constraintStart_toEndOf="@+id/textView39"
                app:layout_constraintTop_toTopOf="@+id/textView39" />

            <ImageView
                android:id="@+id/imageView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_16"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/tvPackageAllDes"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/tvPackageAllName"
                app:srcCompat="@drawable/ic_group_6887" />

            <TextView
                android:id="@+id/tvoutofstock"
                style="@style/normaltxt_12sp_Semibold"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="@dimen/dp_10"
                android:gravity="center_horizontal"
                android:text="Out of Stock"
                android:textColor="@color/pink"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/imageView18"
                app:layout_constraintStart_toStartOf="@+id/imageView18"
                app:layout_constraintTop_toBottomOf="@+id/imageView18" />

            <TextView
                android:id="@+id/textView196"
                style="@style/normaltxt_14sp_Medium"
                android:layout_width="@dimen/dp_73"
                android:layout_height="@dimen/dp_30"
                android:layout_marginEnd="16dp"
                android:background="@drawable/blue_stroke_bg"
                android:gravity="center"
                android:text="Add"
                android:textColor="@color/darknavyblue2"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/textView31"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvPackageAllPrice" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>